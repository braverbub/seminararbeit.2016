@article{Nishino.2016,
 abstract = {The reaction of paprika carotenoids, capsanthin and capsorubin, with reactive oxygen species (ROS), such as superoxide anion radical (.O2(-)), hydroxyl radical (.OH), and singlet oxygen ((1)O2), was analyzed by LC/PDA ESI-MS and ESR spectrometry. Capsanthin formed both the 5,6-epoxide and 5,8-epoxide by reaction with .O2(-) and .OH. Furthermore, capsanthin also formed 5,6- and 5,8-endoperoxide on reaction with (1)O2. The same results were obtained in the case of capsanthin diacetate. On the other hand, capsorubin showed higher stability against these ROS. Capsorubin formed 7,8-epoxide on reaction with .O2(-) and .OH and 7,8-endoperoxide on reaction with (1)O2.},
 author = {Nishino, Azusa and Yasui, Hiroyuki and Maoka, Takashi},
 year = {2016},
 title = {Reaction of Paprika Carotenoids, Capsanthin and Capsorubin, with Reactive Oxygen Species},
 pages = {4786--4792},
 volume = {64},
 number = {23},
 issn = {0021-8561},
 journal = {Journal of agricultural and food chemistry},
 doi = {10.1021/acs.jafc.6b01706}
}


